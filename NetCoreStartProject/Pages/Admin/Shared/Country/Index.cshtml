@page
@model NetCoreStartProject.Pages.Admin.Shared.Country.IndexModel

<p>
    <a asp-page="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Country[0].Code)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Country[0].Latitude)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Country[0].Longitude)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Country[0].FlagCode)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Country[0].CapitalNames)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Country[0].CurrencyCodes)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Country[0].PhoneCodes)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Country[0].alpha2)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Country[0].alpha3)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Country[0].NameAr)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Country[0].NameEn)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Country[0].CreatedByUser)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Country[0].IsDeleted)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.Country) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Code)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Latitude)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Longitude)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FlagCode)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CapitalNames)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CurrencyCodes)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PhoneCodes)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.alpha2)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.alpha3)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NameAr)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NameEn)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IsDeleted)
            </td>
            <td>
                <a asp-page="./Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-page="./Details" asp-route-id="@item.Id">Details</a> |
                <a asp-page="./Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
